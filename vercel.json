
{
  "version": 2,
  "name": "metapassiveincome-final",
  "rewrites": [
    {
      "source": "/api/pay/(.*)",
      "destination": "/api/main"
    },
    {
      "source": "/product/(.*)",
      "destination": "/outputs/$1/index.html"
    },
    {
      "source": "/checkout/(.*)",
      "destination": "/outputs/$1/checkout.html"
    }
  ]
}
